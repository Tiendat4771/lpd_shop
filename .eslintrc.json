{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-console": "off",
    "comma-dangle": "off",
    "no-underscore-dangle": [
      "error",
      { "allow": ["__REDUX_DEVTOOLS_EXTENSION__"] }
    ],
    "react/jsx-filename-extension": "off",
    "prefer-destructuring": [
      "error",
      {
        "array": false,
        "object": false
      },
      {
        "enforceForRenamedProperties": false
      }
    ]
  }
}
